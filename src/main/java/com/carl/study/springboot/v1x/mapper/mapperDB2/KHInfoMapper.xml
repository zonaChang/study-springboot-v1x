<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.carl.study.springboot.v1x.mapper.mapperDB2.KHInfoMapper">


    <select id="getKHInfoById" resultType="kHInfo">
        SELECT t.id, t.name, t.idno FROM t_stkkh_cust_info t WHERE t.id=#{id}
    </select>

</mapper>